{
  "expo": {
    "name": "MovieFlix",
    "slug": "mobile-movie-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/movieflix-logo.png",
    "scheme": "movies",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/movieflix-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kyleddj.donewithit"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/movieflix-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/movieflix-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1145b526-33d3-45a0-9c9e-b2eebda31bb1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1145b526-33d3-45a0-9c9e-b2eebda31bb1"
    }
  }
}
